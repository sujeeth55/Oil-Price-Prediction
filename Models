import numpy as np
import pandas as pd
import warnings
warnings.filterwarnings("ignore")
import prophet
from sklearn.metrics import mean_squared_error
import math
from prophet import Prophet
import pickle
df=pd.read_csv("C:/Users/divya/Downloads/bent oil price.csv", parse_dates=True)
df.rename(columns={"Date":"ds", "Closing Value":"y"}, inplace=True)
df.ds=pd.to_datetime(df.ds)
train=df.iloc[:len(df)-365]
test=df.iloc[len(df)-365:]
model_fb = Prophet()
model_fb.fit(train)

pickle.dump(model_fb,open('model.pkl', 'wb'))
